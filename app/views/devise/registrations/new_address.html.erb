<h2>住所情報登録</h2>

<%= form_for @address do |f| %>
  <%= render "devise/shared/error_messages", resource: @address %>

  <div class="field">
    <%= f.label :zipcode %><br />
    <%= f.text_field :zipcode %>
  </div>

  <div class="field">
    <%= f.label :prefecture %><br />
    <%= f.text_field :prefecture %>
  </div>
    <div class="field">
    <%= f.label :first_address %><br />
    <%= f.text_field :first_address %>
  </div>
    <div class="field">
    <%= f.label :second_address %><br />
    <%= f.text_field :second_address %>
  </div>
    <div class="field">
    <%= f.label :third_address %><br />
    <%= f.text_field :third_address %>
  </div>
    <div class="actions">
    <%= f.submit "Next" %>
  </div>

<% end %>
<%= render "devise/shared/links" %>